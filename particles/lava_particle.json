{
  "format_version": "1.21.60",
  "particle_effect": {
    "description": {
      "identifier": "minecraft:lava_particle",
      "basic_render_parameters": {
        "material": "particles_alpha",
        "texture": "textures/particle/lava_ember_atlas"
      }
    },
    "events": {
      "smoke": {
        "particle_effect": {
          "effect": "minecraft:basic_smoke_particle",
          "type": "particle_with_velocity"
        }
      }
    },
    "components": {
      "minecraft:emitter_initialization": {
        "creation_expression": "v.launch_speed = math.random(3.5, 7.0); v.spin_dir = v.particle_random_3 > 0.5 ? 1 : -1; v.ember_lifetime = math.random(0.6, 2.2);"
      },
      "minecraft:emitter_rate_instant": {
        "num_particles": 3
      },
      "minecraft:emitter_lifetime_once": {},
      "minecraft:emitter_shape_point": {
        "direction": [
          "math.random(-0.14, 0.14)",
          "math.random(0.4, 1.0)",
          "math.random(-0.14, 0.14)"
        ]
      },
      "minecraft:particle_initial_speed": "math.random(3.5, 7.0)",
      "minecraft:particle_initial_spin": {
        "rotation": "v.particle_random_2 * 360",
        "rotation_rate": "v.spin_dir * math.random(40, 120)"
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": "0.6 + v.particle_random_1 * 1.6"
      },
      "minecraft:particle_motion_dynamic": {
        "linear_acceleration": [
          "v.particle_random_1 * 4.0 - 2.0",
          -12.0,
          "v.particle_random_2 * 4.0 - 2.0"
        ],
        "rotation_acceleration": "v.spin_dir * math.lerp(30, 5, math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1))"
      },
      "minecraft:particle_motion_collision": {
        "collision_drag": 40.0,
        "coefficient_of_restitution": 0.06,
        "collision_radius": 0.06
      },
      "minecraft:particle_lifetime_events": {
        "timeline": {
          "0.00": "smoke",
          "0.05": "smoke",
          "0.10": "smoke",
          "0.18": "smoke",
          "0.28": "smoke",
          "0.40": "smoke",
          "0.55": "smoke",
          "0.75": "smoke",
          "1.00": "smoke",
          "1.30": "smoke",
          "1.60": "smoke",
          "1.90": "smoke"
        }
      },
      "minecraft:particle_appearance_billboard": {
        "size": [
          "math.lerp(0.10 + v.particle_random_3 * 0.10, 0.018, math.pow(math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1), 1.5))",
          "math.lerp(0.10 + v.particle_random_3 * 0.10, 0.018, math.pow(math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1), 1.5))"
        ],
        "facing_camera_mode": "lookat_xyz",
        "uv": {
          "texture_width": 128,
          "texture_height": 32,
          "uv": [
            "math.clamp(math.floor(v.particle_random_4 * 4), 0, 3) * 32",
            0
          ],
          "uv_size": [ 32, 32 ]
        }
      },
      "minecraft:particle_appearance_lighting": {},
      "minecraft:particle_appearance_tinting": {
        "color": [
          "1.0",
          "math.lerp(0.95, 0.08, math.pow(math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1), 1.4))",
          "math.lerp(0.55, 0.0, math.pow(math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1), 0.9))",
          "math.lerp(1.0, 0.0, math.pow(math.clamp(v.particle_age / (0.6 + v.particle_random_1 * 1.6), 0, 1), 2.8)) * math.clamp(v.particle_age / 0.04, 0, 1)"
        ]
      }
    }
  }
}
